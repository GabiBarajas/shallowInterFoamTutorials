    {
        type            zeroGradient;
    }
    frontOutSides2d
    {
        type            zeroGradient;
    }
    backOut2d
    {
        type            zeroGradient;
    }
    rightOut2d
    {
        type            zeroGradient;
    }
    leftOut2d
    {
        type            zeroGradient;
    }
    frontIn2d
    {
        type            zeroGradient;
    }
    backIn2d
    {
        type            zeroGradient;
    }
    leftIn2d
    {
        type            zeroGradient;
    }
    rightIn2d
    {
        type            zeroGradient;
    }
    bottom2d
    {
        type            empty;
    }
    top2d
    {
        type            empty;
    }
}


